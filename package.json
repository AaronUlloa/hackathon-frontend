    "lru-cache": "^10.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "chokidar-cli": "^3.0.0",
    "esm": "^3.2.25",
    "glob": "^10.3.4",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-webp": "^8.0.0",
    "postcss-cli": "^10.1.0",
    "sass": "^1.66.1",
    "sharp": "^0.32.5",
    "terser": "^5.19.4"
